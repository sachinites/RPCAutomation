	<member name="emp_strength" dataType="INT32" description="Employee count"/>
	<member name="CEO" dataType="OBJECT" isPTR="true" referredObject="person_t" description="CEO"/>
</C_Structure>

<C_Structure name="person_t" description="person_t C structure">
	<member name="name" dataType="CHARARRAY" cDataArraySize="32" description="name of a person"/>
	<member name="age" dataType="INT32" isPTR="false" description="Age of a person"/>
	<member name="height" dataType="INT32" isPTR="true" cDataArraySize="32" description="Age of a person"/>
	<member name="occupation" dataType="CHARARRAY" cDataArraySize="32" description="Occupation of a person"/>
	<member name="manager" dataType="OBJECT" isPTR="true" referredObject="person_t" description="Manager Details"/>
	<member name="peon" dataType="OBJECT" isPTR="true" referredObject="person_t" cDataArraySize="32" description="Manager Details"/>
	<member name="company" dataType="OBJECT" referredObject="company_t" description="Company Details"/>
	<member name="last_employers" dataType="OBJECT" isPTR="true" referredObject="company_t" vector="true" description="last_employment details"/>
	<member name ="colleagues" dataType="OBJECT" isPTR="true" referredObject="person_t" vector="true"/>
</C_Structure>
